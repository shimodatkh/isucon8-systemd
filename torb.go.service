[Unit]
Description = isucon8 qualifier webapp in go

[Service]
WorkingDirectory=/home/isucon/torb/webapp/go
EnvironmentFile=/home/isucon/torb/webapp/env.sh

ExecStart = /bin/sh -c 'exec /home/isucon/torb/webapp/go/torb > /var/log/torb/app.log 2>&1'

Restart   = always
Type      = simple
User      = isucon
Group     = isucon

[Install]
WantedBy = multi-user.target
